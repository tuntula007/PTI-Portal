    <!--You need to add a reference to Alerts.Email.Settings.EmailSmtpSettingsHandler.dll since this dll lives outside this (Presentation) assembly-->
    <!--Comments can go here-->

    <!--SSL example.  Google(gmail)Mail is a good (free) example of this.  Naturally, you need to provide a legitimate username and password-->
    <!--Note, with the 1.1 Framework, my tests show that gmail likes port 465.  With 2.0, it likes 587-->
    <smtpServer enabled="true" smtpServerName="smtp.gmail.com" defaultEmailFrom="noreply@pti.edu.ng" portNumber="587" authenicationMode="SSL" smtpUserName="noreply@pti.edu.ng" smtpUserPassword="password" executeOrder="3"/>

    <!--Basic authentication.  Passing in a username (and sometimes a password) are used here.-->
