    <!--None authentication.  Nothing but the smtp-server name is provided-->
    <smtpServer enabled="false" smtpServerName="smtp.gmail.com"  authenicationMode="none"  executeOrder="1"/>

  </EmailSettingsSectionName>


	
	
	
	
	
	
	
	
	
